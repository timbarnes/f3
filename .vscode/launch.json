    "version": "0.2.0",
    "MIDebuggerPath": "/usr/bin/lldb",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'f3'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=f3",
                    "--package=f3",
                    "--",
                ],
                "filter": {
                    "name": "f3",
                    "kind": "bin"
                }
            },
            //"args": [],
            "cwd": "${workspaceFolder}/target/debug",
        },
    ]
}